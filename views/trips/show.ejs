<div class="nav">
  <a href="/"> MAIN PAGE </a> 
  <a href="/profile"> PROFILE PAGE </a>
  <a href="/parks"> PARKS PAGE </a> 
  <a href="/trips"> MY TRIPS PAGE </a> 
</div>


<h1 class="titles">Trip Destination: <%= trip.destination %></h1>

<h4 class="titles">Attractions to visit: </h4>
<% trip.parks.forEach(function(park) { %>
<div>
  <a href="/parks/<%= park.id %>"><%= park.name %></a> 
  <form method="POST" action="/parks/<%= park.id %>?_method=DELETE">
    <input class="delete" type="submit" value="DELETE PARK">
  </form>
</div>
<% }); %>

<div class="options">
  <a href="/parks?states=<%= trip.destination %>">Browse More Parks in <%= trip.destination %></a>
  <form method="POST" action="/trips/<%= trip.id %>?_method=DELETE">
    <input class="delete" type="submit" value="DELETE TRIP">
  </form>
</div>

<p class="nav"><a href="javascript:history.go(-1)" title="Return to previous page">&laquo;Go back</a></p>